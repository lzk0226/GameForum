<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.purchase.mapper.BizCdkProductMapper">
    
    <resultMap type="BizCdkProduct" id="BizCdkProductResult">
        <result property="productId"    column="product_id"    />
        <result property="productName"    column="product_name"    />
        <result property="productDescription"    column="product_description"    />
        <result property="gameId"    column="game_id"    />
        <result property="productType"    column="product_type"    />
        <result property="originalPrice"    column="original_price"    />
        <result property="salePrice"    column="sale_price"    />
        <result property="discountRate"    column="discount_rate"    />
        <result property="stockCount"    column="stock_count"    />
        <result property="soldCount"    column="sold_count"    />
        <result property="platform"    column="platform"    />
        <result property="region"    column="region"    />
        <result property="autoDelivery"    column="auto_delivery"    />
        <result property="sortOrder"    column="sort_order"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectBizCdkProductVo">
        select product_id, product_name, product_description, game_id, product_type, original_price, sale_price, discount_rate, stock_count, sold_count, platform, region, auto_delivery, sort_order, status, del_flag, create_by, create_time, update_by, update_time, remark from biz_cdk_product
    </sql>

    <select id="selectBizCdkProductList" parameterType="BizCdkProduct" resultMap="BizCdkProductResult">
        <include refid="selectBizCdkProductVo"/>
        <where>  
            <if test="productName != null  and productName != ''"> and product_name like concat('%', #{productName}, '%')</if>
            <if test="productDescription != null  and productDescription != ''"> and product_description = #{productDescription}</if>
            <if test="gameId != null "> and game_id = #{gameId}</if>
            <if test="productType != null  and productType != ''"> and product_type = #{productType}</if>
            <if test="originalPrice != null "> and original_price = #{originalPrice}</if>
            <if test="salePrice != null "> and sale_price = #{salePrice}</if>
            <if test="discountRate != null "> and discount_rate = #{discountRate}</if>
            <if test="stockCount != null "> and stock_count = #{stockCount}</if>
            <if test="soldCount != null "> and sold_count = #{soldCount}</if>
            <if test="platform != null  and platform != ''"> and platform = #{platform}</if>
            <if test="region != null  and region != ''"> and region = #{region}</if>
            <if test="autoDelivery != null  and autoDelivery != ''"> and auto_delivery = #{autoDelivery}</if>
            <if test="sortOrder != null "> and sort_order = #{sortOrder}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectBizCdkProductByProductId" parameterType="Long" resultMap="BizCdkProductResult">
        <include refid="selectBizCdkProductVo"/>
        where product_id = #{productId}
    </select>

    <insert id="insertBizCdkProduct" parameterType="BizCdkProduct" useGeneratedKeys="true" keyProperty="productId">
        insert into biz_cdk_product
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="productName != null and productName != ''">product_name,</if>
            <if test="productDescription != null">product_description,</if>
            <if test="gameId != null">game_id,</if>
            <if test="productType != null and productType != ''">product_type,</if>
            <if test="originalPrice != null">original_price,</if>
            <if test="salePrice != null">sale_price,</if>
            <if test="discountRate != null">discount_rate,</if>
            <if test="stockCount != null">stock_count,</if>
            <if test="soldCount != null">sold_count,</if>
            <if test="platform != null and platform != ''">platform,</if>
            <if test="region != null">region,</if>
            <if test="autoDelivery != null">auto_delivery,</if>
            <if test="sortOrder != null">sort_order,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="productName != null and productName != ''">#{productName},</if>
            <if test="productDescription != null">#{productDescription},</if>
            <if test="gameId != null">#{gameId},</if>
            <if test="productType != null and productType != ''">#{productType},</if>
            <if test="originalPrice != null">#{originalPrice},</if>
            <if test="salePrice != null">#{salePrice},</if>
            <if test="discountRate != null">#{discountRate},</if>
            <if test="stockCount != null">#{stockCount},</if>
            <if test="soldCount != null">#{soldCount},</if>
            <if test="platform != null and platform != ''">#{platform},</if>
            <if test="region != null">#{region},</if>
            <if test="autoDelivery != null">#{autoDelivery},</if>
            <if test="sortOrder != null">#{sortOrder},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateBizCdkProduct" parameterType="BizCdkProduct">
        update biz_cdk_product
        <trim prefix="SET" suffixOverrides=",">
            <if test="productName != null and productName != ''">product_name = #{productName},</if>
            <if test="productDescription != null">product_description = #{productDescription},</if>
            <if test="gameId != null">game_id = #{gameId},</if>
            <if test="productType != null and productType != ''">product_type = #{productType},</if>
            <if test="originalPrice != null">original_price = #{originalPrice},</if>
            <if test="salePrice != null">sale_price = #{salePrice},</if>
            <if test="discountRate != null">discount_rate = #{discountRate},</if>
            <if test="stockCount != null">stock_count = #{stockCount},</if>
            <if test="soldCount != null">sold_count = #{soldCount},</if>
            <if test="platform != null and platform != ''">platform = #{platform},</if>
            <if test="region != null">region = #{region},</if>
            <if test="autoDelivery != null">auto_delivery = #{autoDelivery},</if>
            <if test="sortOrder != null">sort_order = #{sortOrder},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where product_id = #{productId}
    </update>

    <delete id="deleteBizCdkProductByProductId" parameterType="Long">
        delete from biz_cdk_product where product_id = #{productId}
    </delete>

    <delete id="deleteBizCdkProductByProductIds" parameterType="String">
        delete from biz_cdk_product where product_id in 
        <foreach item="productId" collection="array" open="(" separator="," close=")">
            #{productId}
        </foreach>
    </delete>
</mapper>